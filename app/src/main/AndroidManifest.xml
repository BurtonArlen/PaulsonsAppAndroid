<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.inboundrx.paulsensbeaconsapp">

    <application
        android:name=".sourceCode.managers.BeaconNotificationManager"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".sourceCode.ui.LandingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".sourceCode.ui.AboutActivity" />
        <activity android:name=".sourceCode.ui.HistoryActivity" />
        <activity android:name=".sourceCode.ui.RewardsActivity" />
        <activity android:name=".sourceCode.ui.UserActivity"></activity>
    </application>

</manifest>